<FASTOAD_model>
  <specifications>
    <MTOW is_input="True">360.0<!--Top-level requirement: Maximum Take Off Weight--></MTOW>
    <acceleration>
      <takeoff is_input="True">2.4525<!--Acceleration during takeoff--></takeoff>
    </acceleration>
    <atmosphere>
      <density is_input="True">1.18<!--Air density--></density>
    </atmosphere>
    <duration>
      <hover is_input="True">25.0</hover>
    </duration>
    <payload>
      <mass>
        <max is_input="True">50.0<!--Payload mass--></max>
      </mass>
    </payload>
  </specifications>
  <optim>
    <variable>
      <beta_pro is_input="True">0.33<!--pitch/diameter ratio of the propeller--></beta_pro>
      <k_D is_input="True">0.99<!--aspect ratio D_in/D_out for the arms--></k_D>
      <k_ND is_input="True">1.2<!--slow down propeller coeffiecient : ND = NDmax / k_ND--></k_ND>
      <k_mb is_input="True">1.0<!--over estimation coefficient on the battery load mass--></k_mb>
      <k_mot is_input="True">1.0<!--over estimation coefficient on the motor torque--></k_mot>
      <k_os is_input="True">3.2<!--over estimation coefficient on the load mass--></k_os>
      <k_speed_mot is_input="True">1.2<!--over estimation coefficient on the motor speed--></k_speed_mot>
      <k_vb is_input="True">1.0<!--over estimation coefficient on the battery voltage--></k_vb>
    </variable>
  </optim>
  <data>
    <ESC>
      <reference>
        <mass is_input="True">0.115<!--ESC mass reference for scaling law--></mass>
        <power is_input="True">3108.0<!--ESC max. power reference for scaling law--></power>
      </reference>
    </ESC>
    <battery>
      <reference>
        <energy is_input="True">260568.0<!--Battery energy reference for scaling law--></energy>
        <mass is_input="True">0.329<!--Battery mass reference for scaling law--></mass>
      </reference>
    </battery>
    <motor>
      <reference>
        <mass is_input="True">0.575<!--Motor mass reference for scaling law--></mass>
        <resistance is_input="True">0.03<!--Motor resistance reference for scaling law--></resistance>
        <torque>
          <coefficient is_input="True">0.03<!--Motor torque coefficient reference for scaling law--></coefficient>
          <friction is_input="True">0.03<!--Motor friction torque reference for scaling law--></friction>
          <max is_input="True">2.817<!--Motor max. torque reference for scaling law--></max>
          <nominal is_input="True">2.32<!--Motor nominal torque reference for scaling law--></nominal>
        </torque>
      </reference>
    </motor>
    <propeller>
      <reference>
        <diameter is_input="True">0.2794<!--Propeller diameter reference for scaling law--></diameter>
        <mass is_input="True">0.015<!--Propeller mass reference for scaling law--></mass>
        <ND>
          <max is_input="True">44.695<!--Max. rotational speed not to be exceeded--></max>
        </ND>
      </reference>
    </propeller>
    <structure>
      <arms>
        <number is_input="True">4.0<!--Number of arms--></number>
        <prop_per_arm is_input="True">1.0<!--Number of propellers per arm (single or coaxial configuration)--></prop_per_arm>
        <material>
          <density is_input="True">1700.0<!--Arms material density--></density>
          <stress>
            <max is_input="True">70000000.0<!--Arms material maximum allowable stress--></max>
          </stress>
        </material>
      </arms>
    </structure>
  </data>
</FASTOAD_model>
